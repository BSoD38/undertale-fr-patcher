{
  "name": "undertale-fr-patcher",
  "productName": "undertale-fr-patcher",
  "version": "1.0.1",
  "description": "French Undertale patch installer",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "build": "electron-builder"
  },
  "build": {
    "appId": "undertale.fr.patcher",
    "win": {
      "target": "portable",
      "extraResources": ["Patches/mus_ohyes.ogg", "Patches/win"]
    },
    "mac": {
      "target": "mas",
      "extraResources": ["Patches/mus_ohyes.ogg", "Patches/ios"]
    },
    "linux": {
      "extraResources": ["Patches/mus_ohyes.ogg", "Patches/unx"]
    }
  },
  "keywords": [],
  "author": "UTFR Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^11.2.0",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "electron-store": "^6.0.1"
  }
}
